{
  "name": "DemoEMR",
  "version": "1.2.6",
  "private": true,
  "repository": "https://github.com/satv0878/DemoEMR",
  "author": "satv0878",
  "description": "A minimalistic EMR system to process HL7 messages and display measurement results as well as patient demographics",
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "gh-publish": "electron-builder --x64 -p always",
    "postinstall": "electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps", 
    "build:android": "expo build:android"
  },
  "main": "background.js",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.18",
    "@fortawesome/free-regular-svg-icons": "^5.8.2",
    "@fortawesome/free-solid-svg-icons": "^5.8.2",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@xkeshi/vue-barcode": "^1.0.0",
    "axios": "^0.18.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.0-rc.21",
    "core-js": "^2.6.5",
    "electron-log": "^3.0.5",
    "electron-updater": "^4.0.6",
    "material-design-icons-iconfont": "^5.0.1",
    "node": "^12.3.0",
    "shelljs": "^0.8.3",
    "simple-hl7": "^3.1.0",
    "update-electron-app": "^1.3.0",
    "vue": "^2.6.10",
    "vue-axios": "^2.1.4",
    "vue-cli": "^2.9.6",
    "vuetify": "^1.5.14",
    "webpack-cli": "^3.3.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.7.0",
    "@vue/cli-plugin-eslint": "^3.7.0",
    "@vue/cli-service": "^3.7.0",
    "babel-eslint": "^10.0.1",
    "electron": "^11.5.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "vue-cli-plugin-electron-builder": "^1.3.2",
    "vue-template-compiler": "^2.5.21"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {},
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
